	"TELE_SALE_NO" VARCHAR2(50 BYTE), 
	"TELE_SALE_FILE" VARCHAR2(255 BYTE), 
	"SALE_RAW_AGRI" CHAR(1 BYTE) DEFAULT 'N', 
	"SALE_PROCESSED" CHAR(1 BYTE) DEFAULT 'N', 
	"SALE_LIVESTOCK" CHAR(1 BYTE) DEFAULT 'N', 
	"SALE_SEAFOOD" CHAR(1 BYTE) DEFAULT 'N', 
	"SALE_OTHER" CHAR(1 BYTE) DEFAULT 'N', 
	"FOOD_BIZ_TYPE" VARCHAR2(50 BYTE), 
	"FOOD_BIZ_NO" VARCHAR2(50 BYTE), 
	"LIVESTOCK_BIZ_TYPE" VARCHAR2(50 BYTE), 
	"LIVESTOCK_BIZ_NO" VARCHAR2(50 BYTE), 
