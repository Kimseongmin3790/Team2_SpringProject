--  DDL for Table USERS
--------------------------------------------------------

  CREATE TABLE "AGRICOLA"."USERS" 
   (	"USER_ID" VARCHAR2(50 BYTE), 
	"EMAIL" VARCHAR2(100 BYTE), 
	"PASSWORD" VARCHAR2(255 BYTE), 
	"NAME" VARCHAR2(50 BYTE), 
	"PHONE" VARCHAR2(20 BYTE), 
	"USER_ROLE" VARCHAR2(20 BYTE), 
	"ADDRESS" VARCHAR2(255 BYTE), 
	"CDATETIME" DATE, 
	"UDATETIME" DATE, 
	"LAT" NUMBER(10,6), 
	"LNG" NUMBER(10,6), 
	"STATUS" VARCHAR2(20 BYTE) DEFAULT 'ACTIVE', 
	"USER_BIRTH" DATE, 
	"USER_GENDER" CHAR(1 BYTE)
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "USERS" ;

   COMMENT ON COLUMN "AGRICOLA"."USERS"."USER_ID" IS 'PK, 정규화 적용';
   COMMENT ON COLUMN "AGRICOLA"."USERS"."EMAIL" IS '로그인용 이메일';
   COMMENT ON COLUMN "AGRICOLA"."USERS"."PASSWORD" IS '해시 적용, 정규화 적용';
   COMMENT ON COLUMN "AGRICOLA"."USERS"."NAME" IS '정규화 적용';
   COMMENT ON COLUMN "AGRICOLA"."USERS"."PHONE" IS '휴대폰 인증 따로 받기';
   COMMENT ON COLUMN "AGRICOLA"."USERS"."USER_ROLE" IS 'BUYER,SELLER,SYSTEM';
   COMMENT ON COLUMN "AGRICOLA"."USERS"."ADDRESS" IS '주소 API 연동';
   COMMENT ON COLUMN "AGRICOLA"."USERS"."CDATETIME" IS 'SYSDATE';
   COMMENT ON COLUMN "AGRICOLA"."USERS"."UDATETIME" IS 'SYSDATE';
   COMMENT ON COLUMN "AGRICOLA"."USERS"."LAT" IS '위도';
   COMMENT ON COLUMN "AGRICOLA"."USERS"."LNG" IS '경도';
   COMMENT ON COLUMN "AGRICOLA"."USERS"."STATUS" IS 'ACTIVE, WITHDRAWN,LOCKED';
   COMMENT ON COLUMN "AGRICOLA"."USERS"."USER_BIRTH" IS '생년월일';
   COMMENT ON COLUMN "AGRICOLA"."USERS"."USER_GENDER" IS '성별 M or F';
